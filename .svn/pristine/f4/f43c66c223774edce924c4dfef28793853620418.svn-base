/* footer menu Style */
#footer{width: 100%;background-color:#F8F8F8;border-top: 1px solid #E1E1E0;height:100px;padding:15px 0;clear:both;}
#mainfooter{width: 950px;margin: 0 auto;position: relative;}
#mainfooter p{display: block;float:left;padding:0px 40px 20px 10px;height:55px;}
.use-help {width: 610px;height:30px;float:left;}
.use-help li{float:left;padding:0 10px;background: url(../images/common/bot_bar.gif) no-repeat right center;}
.use-help li.nobar{background:none;}
.add{width: 590px;float:left;padding-left:10px;}
.add ul{width: 590px;}
.add ul li{color:#55595F; display: block;width: 590px;line-height:20px;font-size: 12px;letter-spacing: 0px;}
#footer .site {float: left;width: 146px;height:22px; right:0px;position:absolute;overflow: visible; top: 0px;z-index: 4;}
