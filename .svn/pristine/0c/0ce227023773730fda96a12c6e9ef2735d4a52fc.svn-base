.pop_contents {position:relative; border: 1px solid #E8E8E8; width:950px; height:500px; background-color:#ffffff; padding:15px;border-radius:.5em; margin:0 auto;}
.pop_contents h4{font:16px NanumGothic, "나눔고딕", sans-serif; font-weight:600; color:#414040; background:url("../images/common/bnr_nxt.gif") no-repeat left 3px; padding-left:18px; border-bottom: 1px solid #E8E8E8;padding-bottom:8px;margin-bottom:7px;}
.pop_contents p{position:absolute;display:block;background:url("../images/sub/close.gif") no-repeat top center;width:20px; height:20px;top:15px; right:22px;font-size:0px;}

/* 팝업테이블02 */
.pop_contents1 {position:relative; border: 1px solid #E8E8E8;width:450px; height:130px; background-color:#ffffff; padding:15px;border-radius:.5em;}
.pop_contents1 h4{font:16px NanumGothic, "나눔고딕", sans-serif; font-weight:600; color:#414040; background:url("../images/common/bnr_nxt.gif") no-repeat left 3px; padding-left:18px; border-bottom: 1px solid #E8E8E8;padding-bottom:8px;margin-bottom:7px;}
.pop_contents1 p{position:absolute;display:block;background:url("../images/sub/close.gif") no-repeat top center;width:20px; height:20px;top:15px; right:22px;font-size:0px;}

/* 팝업테이블 유저 */
.pop_device{
	text-align: center;
    width: 750px;
    display: block;
    width:100%; 
    border-collapse:separate; 
    font:13px NanumGothic, "나눔고딕", sans-serif;
}
.pop_device tbody{
	position:relative;
	display: block;
    width: 750px;
    overflow: auto;
    height: 360px;
}
.pop_device, th, td{border:0;text-align:center;}
.pop_device th{padding:6px 8px;  border-top:1px solid #e1e4e5; border-right: 1px solid #E8E8E8;border-bottom:1px solid #DBDBDB; background-color:#F4F3F3;font-weight:600;height:26px;}
.pop_device td{height:32px; line-height:23px;padding:3px 8px;  border-bottom:1px solid #DBDBDB; background: #ffffff;}
.pop_device td p{text-align:right;}

.pop_user th:nth-of-type(1), .pop_user td:nth-of-type(1) { width: 70px; }
.pop_user th:nth-of-type(2), .pop_user td:nth-of-type(2) { width: 190px; }
.pop_user th:nth-of-type(3), .pop_user td:nth-of-type(3) { width: 190px; }
.pop_user th:nth-of-type(4), .pop_user td:nth-of-type(4) { width: 60px; }
.pop_user th:nth-of-type(5), .pop_user td:nth-of-type(5) { width: 100px; }
.pop_user th:nth-of-type(6), .pop_user td:nth-of-type(6) { width: 100px; }
.pop_user th:last-child { width: 70px; border-right:0px solid #E8E8E8;}
.pop_user td:last-child { width: calc( 70px - 19px );  border-right:0px solid #E8E8E8;}

.pop_disp th:nth-of-type(1), .pop_disp td:nth-of-type(1) { width: 60px; }
.pop_disp th:nth-of-type(2), .pop_disp td:nth-of-type(2) { width: 200px; }
.pop_disp th:nth-of-type(3), .pop_disp td:nth-of-type(3) { width: 80px; }
.pop_disp th:nth-of-type(4), .pop_disp td:nth-of-type(4) { width: 100px; }
.pop_disp th:nth-of-type(5), .pop_disp td:nth-of-type(5) { width: 130px; }
.pop_disp th:last-child { width: 180px; border-right:0px solid #E8E8E8;}
.pop_disp td:last-child { width: calc( 180px - 19px );  border-right:0px solid #E8E8E8;}
