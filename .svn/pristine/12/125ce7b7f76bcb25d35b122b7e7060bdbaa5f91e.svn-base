/*04공지사항뷰페이지 */

.board_title {   
    font-size: 18px;
    font-weight: 600;
    margin: 0;
    padding: 0 10px 10px 0px;
    text-align: center;
}
.board_view_infor {

    border-bottom: 1px solid #e4e4e4;
    padding: 5px 0;
}
.board_view_infor::after {
    clear: both;
    content: "";
    display: block;
}
.board_view_infor li {
    float: left;
    font-size: 13px;
    margin-bottom: 5px;
    padding: 0;
    width: 50%;
	line-height:25px;
}


.board_view_infor li::after {
    clear: both;
    content: "";
    display: block;
}
.btn_box{line-height:30px;}
.board_view_infor li span {
    /*background: url("../images/sub/page_bul2.gif") no-repeat scroll right 5px rgba(0, 0, 0, 0);*/
    display: inline-block;
    float: left;
    font-weight: 600;
    margin-right: 10px;
    padding: 0 10px 0 0px;
    vertical-align: top;
    width: 60px;
}
.board_view_infor li.none {
    float: none;
    width: 100%;
}
.board_view_text {
    border-bottom: 1px solid #e4e4e4;
    padding: 10px;
}
.board_view_text .url_infor {
    color: #b4b3b3;
    font-size: 12px;
    line-height: 15px;
    margin: 30px 0 0;
    text-align: right;
}
.board_view_infor li .file {
    float: left;
    padding: 0;
    width: 90%;
}
.board_view_infor li .file li {
    float: none;
    margin-bottom: 0;
    padding: 0;
    width: 100%;
}
.board_view_infor li .file li a {
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: top;
    white-space: nowrap;
    width: 100%;
}

.paginate_list {
    border-bottom: 1px solid #898989;
    border-top: 1px solid #898989;
    margin: 30px 0 0;
    width: 100%;
}

.paginate_list li:first-child {
    border-top: 0 none;
}
.paginate_list li {
    background: none repeat scroll 0 0 #fbfbfb;
    border-top: 1px solid #e4e4e4;
    padding: 10px 0;
    position: relative;
}

.paginate_list li span.prev {
    background: url("../images/sub/page_bul.gif") no-repeat scroll 10px 8px rgba(0, 0, 0, 0);
    font-weight: 600;
    padding: 0 10px 0 25px;
    position: absolute;
    width: 40px;
}

.paginate_list li span.subject {
    /*background: url("../images/sub/page_bul2.gif") no-repeat scroll left 5px rgba(0, 0, 0, 0);*/
    margin-left: 75px;
    padding: 0 15px;
    width: 68%;
}

.paginate_list li span.next {
    background: url("../images/sub/page_bul.gif") no-repeat scroll 10px -35px rgba(0, 0, 0, 0);
    font-weight: 600;
    padding: 0 10px 0 25px;
    position: absolute;
    width: 40px;
}

.paginate_list li span.subject {
    /*background: url("../../images/kor/board/page_bul2.gif") no-repeat scroll left 5px rgba(0, 0, 0, 0);*/
    margin-left: 75px;
    padding: 0 15px;
    width: 68%;
}
.paginate_list li span {
    display: inline-block;
    vertical-align: top;
}

