.contents_3d li{float:left; width:240px; height:240px; overflow:hidden; cursor:pointer;} /* 237 */
/*
.contents_3d li:first-child{cursor:default;}*/

.contents_3d:after{content:""; float:none; clear:both; display:block;}
.contents_3d li div{width:240px; height:240px; background-size:auto 100%; background-repeat:no-repeat; background-position: top center; text-align:center;}
/*
.contents_3d li:nth-child(3){background-image: url('../img/3dcontents_1.jpg');}
.contents_3d li:nth-child(5){background-image: url('../img/3dcontents_2.jpg');}
.contents_3d li:nth-child(10){background-image: url('../img/3dcontents_3.jpg');}
.contents_3d li:nth-child(12){background-image: url('../img/3dcontents_4.jpg');}*/
.contents_3d li div:hover{
	transform:scale(1.2,1.2);
	-webkit-transform:scale(1.1);
	-moz-transform:scale(1.1);
	-ms-transform:scale(1.1);
	-o-transform:scale(1.1);
	-webkit-transition:1s ease;
	-moz-transition:1s ease;
	-ms-transition:1s ease;
	-o-transition:1s ease;
	 opacity: 0.8;
    filter: alpha(opacity=80); /* For IE8 and earlier */
}
.contents_3d li div{text-align:center;position:relative;}
.contents_3d li p.copy{position:absolute; top:34px; left:40px; width:166px; height:108px; padding-top:54px; font-weight:bold; text-align:center;
	font:32px NotoSansKR-Regular-Hestia;
	color:#000;
	background:rgba(255, 255, 255, 0.8);
	border:2px solid #fff;
	display:inline-block;
}
.contents_3d li p.copyline3{padding-top:58px; padding-bottom:26px;}
.contents_3d li p.copy span{width:22px; height:5px; display:block; background:#000; margin:0 auto;}
.contents_3d_left{position:absolute; left:-240px; top:240px;}
.contents_3d_right{position:absolute; right:-238px; top:480px;}

/* 3dprint */
.print_3d_wrap{}
.print_3d_title_dot{width:56px; height:6px; background:#000; margin:41px 0 0 2px;}
.print_3d_title{font:37px NotoSansKR-Bold-Hestia; color:#000; text-align:left;}
.print_3d_title span{font-family: 'NotoSansKR-Regular-Hestia';}
.print_3d{margin-bottom:42px;}
.print_3d:after{content:""; float:none; clear:both; display:block;}
.print_3d li{float:left; width:240px; height:240px; overflow:hidden;}
.print_3d li:nth-child(1){width:480px; height:240px;}
.print_3d li div{width:240px; height:240px; background-size:auto 100%; background-repeat:no-repeat; background-position: top center; text-align:center;}
.print_3d li:nth-child(1) div{width:480px; background-size:100% auto; background-position: center center;}

.print_3d li div:hover{
	transform:scale(1.2,1.2);
	-webkit-transform:scale(1.1);
	-moz-transform:scale(1.1);
	-ms-transform:scale(1.1);
	-o-transform:scale(1.1);
	-webkit-transition:1s ease;
	-moz-transition:1s ease;
	-ms-transition:1s ease;
	-o-transition:1s ease;
	 opacity: 0.8;
    filter: alpha(opacity=80);
}

.print_3d li:last-child{width:480px; height:240px;}
.print_3d li:last-child div{width:480px; height:240px; background-size:100% auto; background-position: center center;}
.print_3d_left{position:absolute; left:-240px; top:240px;}
.print_3d_right1{position:absolute; right:-238px; top:0px;}
.print_3d_right2{position:absolute; right:-238px; top:480px;}

