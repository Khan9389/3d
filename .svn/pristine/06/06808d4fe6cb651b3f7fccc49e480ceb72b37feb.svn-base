package com.asia3d.util;


public class JspUtil {
	/**
	 * 유저 타입을 한글로 반환
	 * @param type
	 * @return
	 */
	public static String getUserType(Object type) {
//		1:개인-준회원,2:개인-정회원,3:기업-준회원,4:기업-정회원
		if(type == null)
			return "";
		
		if (type.equals("1"))
			return "개인-준회원";
		else if (type.equals("2"))
			return "개인-정회원";
		else if (type.equals("3"))
			return "기업-준회원";
		else if (type.equals("4"))
			return "개인-정회원";

		return "";
	}
	
}
