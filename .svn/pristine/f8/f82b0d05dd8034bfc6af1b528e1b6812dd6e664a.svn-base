@charset "utf-8";

/* reset.css */
*{margin:0; padding:0;}
ul, ol, dl, dt, dd{list-style:none; margin:0; padding:0;}
fieldset{border:none;}
img{vertical-align: middle;
	border: 0px;
}
input, textarea, select, label, button{vertical-align:middle;}
address, em, strong{font-style:normal; font-weight:normal;}
q{quotes:"" "";}
q:before, q:after{content:"";}
hr{display:none}
button{cursor: pointer; border:0; background-color:transparent;}
h1,h2,h3,h4,h5,h6{
	margin: 0px;
	padding: 0px;}
input[type="submit"] {
    cursor: pointer;
}
input[type="file"] {
    align-items: baseline;
	border:0;
	cursor: pointer;
}
input[type="radio"] {
    align-items: baseline;
	margin-right:5px;
}
/* input[type="text"]::-webkit-input-placeholder{color: blue; } 
input[type="text"]::-moz-placeholder{color: blue; }*/
input[type="text"], input[type="password"] {
    border: 1px solid #dfdfdf;
    background: #fff;
    padding: 3px 5px 2px 5px;
    height: 22px;
	line-height:16px;
}
input[type="button"]{
	cursor: pointer;
	vertical-align: middle;
}
select{width:120px;
	height: 28px;
    margin-right: 5px;
    padding: 4px 5px;
    vertical-align: middle;
	background: #fff none repeat scroll 0 0 !important;
    border: 1px solid #e5e5e5;
    border-radius: 0.3em !important;
    color: #333;
    font-family: "맑은 고딕";
    font-size: 12px;   
}
.loding{height:40px;width:100% text-align:center;claer:both;float:left;}

