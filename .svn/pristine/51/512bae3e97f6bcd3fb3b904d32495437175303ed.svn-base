@font-face {
  font-family: 'NotoSansKR-Light-Hestia';
  src: url('fonts/NotoSansKR-Light-Hestia.eot');
  src: url('fonts/NotoSansKR-Light-Hestia.woff') format('woff'),
       url('fonts/NotoSansKR-Light-Hestia.otf') format('truetype'),
       url('fonts/NotoSansKR-Light-Hestia.svg#NotoSansKR-Bold-Hestia') format('svg'),
       url('fonts/NotoSansKR-Light-Hestia.eot?#iefix') format('embedded-opentype');
  font-weight: normal;
  font-style: normal;
}

